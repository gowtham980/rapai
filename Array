inlist=[]
n=int(input("Enter size of the list:\n"))
 
for i in range(0,n):
    print("Select the number between 0 -",n)
    temp=int(input())
    inlist.append(temp)
print(inlist)
print(len(inlist))
for i in range(len(inlist)):
    if i in inlist:
        if inlist.count(inlist[i])>1:
            print(inlist[i],"-",inlist.count(inlist[i]))
